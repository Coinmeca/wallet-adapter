{
    "name": "@coinmeca/wallet-adapter",
    "description": "Coinmeca wallet adapter for Multichain supporting.",
    "version": "0.1.47",
    "keywords": [
        "coinmeca",
        "web3",
        "wallet",
        "adapter",
        "wallet adapter"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/coinmeca/wallet-adapter"
    },
    "author": "Coinmeca Team <dev@coinmeca.net>",
    "license": "MIT",
    "scripts": {
        "watch": "tsc --watch",
        "build": "tsc --build && tsc-alias && tscp",
        "deploy": "npm publish",
        "clean": "rimraf ./dist ./build ./tsconfig.tsbuildinfo ./node_modules/.cache",
        "prebuild": "yarn clean"
    },
    "sideEffects": false,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typings": "./dist/index.d.js",
    "files": [
        "./dist"
    ],
    "exports": {
        ".": "./dist/index.js",
        "./*": [
            "./dist/*.js",
            "./dist/*/index.js"
        ],
        "./evm": "./dist/core/evm/index.js",
        "./evm/*": [
            "./dist/core/evm/*.js",
            "./dist/core/evm/*/index.js"
        ],
        "./svm": "./dist/core/svm/index.js",
        "./svm/*": [
            "./dist/core/svm/*.js",
            "./dist/core/svm/*/index.js"
        ]
    },
    "typesVersions": {
        "*": {
            ".": [
                "./dist/index"
            ],
            "*": [
                "./dist/*",
                "./dist/*/index",
                "./dist/index"
            ]
        }
    },
    "dependencies": {
        "@babel/runtime": "^7.23.8",
        "eventemitter3": "^5.0.1",
        "lodash": "^4.17.21",
        "node-fetch": "^3.3.2",
        "zustand": "^4.5.0"
    },
    "optionalDependencies": {
        "@coinbase/wallet-sdk": "^3.9.1",
        "@metamask/detect-provider": "^2.0.0",
        "@metamask/sdk": "^0.15.0",
        "@rainbow-me/rainbowkit": "^2.0.1",
        "@solana/wallet-adapter-wallets": "^0.19.25"
    },
    "devDependencies": {
        "@solana/web3.js": "^1.95.3",
        "@types/lodash": "^4.14.202",
        "tsc-alias": "^1.8.8",
        "tscp": "^1.0.0",
        "typescript": "^5.3.3",
        "typescript-cp": "^0.1.9"
    },
    "packageManager": "yarn@4.3.1"
}
